	<string>SPOTIFY_CLIENT_ID_GOES_HERE</string>

	<!-- Optional privacy descriptions -->
	<key>NSLocalNetworkUsageDescription</key>
	<string>Sortify uses a local callback URL to complete Spotify sign-in.</string>
</dict>
</plist>
